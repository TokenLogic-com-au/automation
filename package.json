{
  "name": "aave-paraswap-fee-claim-bot",
  "version": "1.0.0",
  "description": "Gelato Web3 Functions To Claim From Paraswap",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn install && yarn compile && npx tsc",
    "compile": "npx hardhat compile --force",
    "clean": "rm -rf dist",
    "deploy": "npx hardhat deploy",
    "format": "prettier --write */**/*.*{js,sol,json,md,ts}",
    "format:check": "prettier --check */**/*.*{js,sol,json,md,ts}",
    "lint": "eslint --cache . && yarn lint:sol",
    "lint:ts": "eslint -c .eslintrc.json --ext \"**/*.ts\" \"**/*.test.ts\"",
    "lint:sol": "solhint contracts/**/*.sol",
    "test": "npx hardhat test",
    "verify": "npx hardhat etherscan-verify",
    "w3f-run": "npx hardhat w3f-run",
    "w3f-deploy": "npx hardhat w3f-deploy"
  },
  "license": "ISC",
  "devDependencies": {
    "@matterlabs/hardhat-zksync-solc": "^0.3.17",
    "@matterlabs/hardhat-zksync-verify": "^0.1.8",
    "@nomicfoundation/hardhat-chai-matchers": "1.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.2",
    "@nomicfoundation/hardhat-network-helpers": "1.0.8",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@openzeppelin/contracts": "4.8.1",
    "@tsconfig/recommended": "1.0.2",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "6.1.5",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethereum-waffle": "4.0.10",
    "ethers": "5.7.2",
    "events": "^3.3.0",
    "hardhat": "^2.13.0",
    "hardhat-deploy": "^0.11.31",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "lint-staged": "13.1.2",
    "prettier": "2.8.4",
    "prettier-plugin-solidity": "1.1.2",
    "process": "^0.11.10",
    "solhint": "3.4.0",
    "solhint-plugin-prettier": "0.0.5",
    "stream": "^0.0.3",
    "ts-generator": "0.1.1",
    "ts-node": "10.9.1",
    "typechain": "8.1.1",
    "typescript": "4.9.5",
    "util": "^0.12.5",
    "zlib": "^1.0.5"
  },
  "lint-staged": {
    "*.*{js,sol,json,md,ts,yml,yaml}": "prettier --write",
    "*.*{ts,js}": "eslint -c .eslintrc.json"
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.8.0",
    "@gelatonetwork/automate-sdk": "^3.0.2",
    "@gelatonetwork/relay-sdk": "^5.5.6",
    "@gelatonetwork/web3-functions-sdk": "2.3.0",
    "@safe-global/protocol-kit": "^6.0.4",
    "@safe-global/relay-kit": "^4.0.3",
    "@safe-global/safe-ethers-adapters": "^0.1.0-alpha.19",
    "@safe-global/types-kit": "^2.0.1",
    "ky": "^0.33.2"
  }
}
